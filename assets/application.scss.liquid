.testimonial {
    display: flex;
    flex-direction: row;
    gap: 30px;
    overflow-x: scroll;
    padding: 2% 4%;
    cursor: grab;

    &::-webkit-scrollbar {
        display: none;
      }
      &::-webkit-scrollbar-track {
        display: none;
      }
      &::-webkit-scrollbar-thumb {
        display: none;
      }
      scrollbar-width: none;

    
    .testimonial-card {
        background-color: #fff;
        border-radius: 20px;
        max-width: 600px;
        display: flex;
       gap: 30px;
        padding: 2%;
        color: #1B1C1D;
        @include phone {
            gap: 10px;
            padding: 3%;
        }
        .review-text {
            display: flex;
            flex-direction: column;
            min-width: 250px;
            gap: 10px;
            @include phone {
                min-width: 100px;
            }
            .name {
                font-family: 'Aeroport';
                font-style: normal;
                font-weight: 700;
                font-size: 20px;
                line-height: 27px;
            }
            .date {
                @include phone {
                    font-size: 13px;
                }
            }
            .review-words {
                font-family: 'Aeroport';
                font-style: normal;
                font-weight: 400;
                font-size: 20px;
                line-height: 27px;
               

                @include phone {
                    font-size: 15px;
                    line-height: 16px;
                    padding-right: 0;
                }

        }

            .starts {
           
            display: flex;
            gap: 10px;
            align-items: center;

             img {
                 height: 15px;
                 width: auto;

            }
        }

       
    }
    .review-image {
        img {
            border-radius: 20px;
            max-height: 210;
            width: auto;

            @include phone {
                width: 150px;
                height: auto;
            }
        }
    }
    }
}


