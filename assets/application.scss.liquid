        url('AeroportMono.woff2') format('woff2'),
        url('AeroportMono.woff') format('woff'),
        url('AeroportMono.ttf') format('truetype'),
        url('AeroportMono.svg#AeroportMono') format('svg');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Aeroport';
    src: url('Aeroport.eot');
    src: url('Aeroport.eot?#iefix') format('embedded-opentype'),
