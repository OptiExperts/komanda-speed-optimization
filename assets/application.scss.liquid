        url('Aeroport.svg#Aeroport') format('svg');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@mixin phone {
    @media  (max-width: 599px) {
        @content;
    }
}

@mixin tablet {
    @media  (max-width: 990px) {
        @content;
    }
}
@mixin desktop {
    @media  (max-width: 1200px) {
        @content;
    }
}


