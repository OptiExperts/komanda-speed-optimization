.product-template {
    .swiper-button-next::before {
        background: url('cursor-arrow-right.svg') 24px center no-repeat;
        content: "";
        opacity: 0.25;
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        right: 0;
        transition: opacity 0.15s;
        background-position: calc(100%-30px);
       
    }
    .swiper-button-prev::before {
        background: url('cursor-arrow-left.svg') 24px center no-repeat;
        content: "";
        opacity: 0.25;
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        left: 0;
        transition: opacity 0.15s;

      
    }
}

.product-image:hover .swiper-button-next::before ,
.product-image:hover .swiper-button-prev::before { 
