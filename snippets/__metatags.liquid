<meta name="viewport" content="width=device-width">
<meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <meta itemprop="image" content="https://cdn.shopify.com/s/files/1/0625/0158/5147/files/kmanda_inst_logo.jpg?v=1670594352">
  <!-- Facebook Meta Tags -->
  <meta property="og:url" content="https://komanda.gallery">
  <meta property="og:type" content="website">
  <meta property="og:title" content="{{ page_title }}">
  <meta property="og:description" content="{{ page_description | escape }}">
  <meta property="og:image" content="http://cdn.shopify.com/s/files/1/0625/0158/5147/files/kmanda_inst_logo.jpg?v=1670594352">

  <!-- Twitter Meta Tags -->
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:title" content="{{ page_title }}">
  <meta name="twitter:description" content="{{ page_description | escape }}">
  <meta name="twitter:image" content="http://cdn.shopify.com/s/files/1/0625/0158/5147/files/kmanda_inst_logo.jpg?v=1670594352">